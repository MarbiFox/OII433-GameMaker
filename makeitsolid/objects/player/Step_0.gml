switch(keyboard_key){
	case vk_up:
	case ord("W"):
		map_acctions[? "up"]();	
	break;
	
	case vk_down:
	case ord("S"):
		map_acctions[? "down"]();
	break;
	
	case vk_left:
	case ord("A"):
		map_acctions[? "left"]();
	break;
	
	case vk_right:
	case ord("D"):
		map_acctions[? "right"]();
	break;	
}

